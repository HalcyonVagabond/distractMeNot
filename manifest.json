{
    "manifest_version": 2,
    "name": "ClearTube",
    "version": "1.0",
  
    "description": "Description here.",
  
    "icons": {
      "48": "icons/clear.jpeg"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],

    "permissions": [
      "webNavigation"
    ],

    "browser_action": {
      "default_icon": "icons/clear.jpeg",
      "default_title": "Distract Me Not",
      "default_popup": "view.html"
    }
  
  }
  