{
    "manifest_version": 2,
    "name": "DistractMeNot",
    "version": "1.0",
  
    "description": "A browser plugin that helps you focus by removing distracting sections of websites. Enjoy!",
  
    "icons": {
      "48": "icons/clear.jpeg"
    },
  
    "background": {
      "scripts": ["background.js"]
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ]
}
  